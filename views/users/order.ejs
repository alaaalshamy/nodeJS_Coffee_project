<div class="container">



    <div class="table-left">
        <input type="date" id="from" name="from" value="15-8-2018"  style="margin-left: 15px;margin-right: 25px">

        <input type="date" id="to" name="to" value="">

        <div style="color: white">
        <table class="table table-dark" style="background-color: #24201e !important">
            <tbody>
            <% for(var i=0; i < result.length; i++) {
            %>
            <tr>
                <td><%= result[i]['date'] %></td>
                <td style="color: green" ><% if (result[i]['status']==0)%>
                    <%= "Process" %>
                    </td>
                <td style="color:#fab347" ><% if (result[i]['process']==0)%>
                    <%= "Done" %>
                </td>
            </tr>
            <% } %>
            </tbody>
        </table>
    </div>
    </div>
        <div class="table-right">
            <% for(var i=0; i < result.length; i++)
            {    for(var j=0;j<result[i].orderItem.length;j++){ %>
            <tr>
                <button class="btn-coffe" price=" ">
                    <p>#<%= result[i].orderItem[j][0] %> <br/>  <%= result[i].orderItem[j][2] %> LE </p>
                    <i class="fa fa-coffee"></i>
                    <p> <%= result[i].orderItem[j][1] %> count </p>

                </button>
            </tr>
            <% }} %>
        </div>
    </div>
</div>
